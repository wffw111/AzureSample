
<div class="form-group col-xs-12 col-lg-6">
    <label class="control-label">Name</label>
    <input type="text" class="form-control" name="Name"
           ng-model="newItem['Name']" />
</div>

<div class="form-group col-xs-12 col-lg-6">
    <label class="control-label">Address</label>
    <input type="text" class="form-control" name="Address"
           ng-model="newItem['Address']" />
</div>


<div class="form-group col-xs-12 col-lg-6">
    <label class="control-label">PostalCode</label>
    <input type="text" class="form-control" name="PostalCode"
           ng-model="newItem['PostalCode']" />
</div>


<div class="form-group col-xs-12 col-lg-6">
    <label class="control-label">Phone</label>
    <input type="text" class="form-control" name="Phone"
           ng-model="newItem['Phone']" />
</div>

<div class="form-group col-xs-12 col-lg-6">
    <label class="control-label">Contact</label>
    <input type="text" class="form-control" name="Contact"
           ng-model="newItem['Contact']" />
</div>

<div class="form-group col-xs-12 col-lg-6">
    <label class="control-label">Establish Date</label>
    <dt-input class="dt-input-group" input-name="EstablishDate"
              input-date="newItem['EstablishDate']">
    </dt-input>
</div>

